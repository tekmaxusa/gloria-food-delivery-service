{
  "name": "gloria-food-delivery-service",
  "version": "1.0.0",
  "description": "TypeScript service to connect with Gloria Food API for delivery orders",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "cli": "ts-node src/cli.ts",
    "webhook": "ts-node src/services/webhook-handler.ts",
    "enhanced-webhook": "ts-node src/services/enhanced-webhook-handler.ts",
    "webhook-manager": "ts-node src/webhook-manager.ts",
    "test-webhook": "ts-node src/test-webhook-system.ts",
    "poll": "ts-node src/cli.ts poll",
